{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs/core"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "outputSourceFiles": true,
    "path": "ink-docstrap",
    "navType": "vertical",
    "linenums": true,
    "collapseSymbols": false,
    "theme": "cosmo",
    "inverseNav": false,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
